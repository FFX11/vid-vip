{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "functions": {
            "controllers/*.js": { "maxDuration": 30 },
            "middleware/**/*.js": { "maxDuration": 30 }
      }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ],
    "installCommand": "npm install"
}